<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>SLogin</servlet-name>
    <servlet-class>Student.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SLogin</servlet-name>
    <url-pattern>/SLogin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SRegister</servlet-name>
    <servlet-class>Student.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SRegister</servlet-name>
    <url-pattern>/SRegister</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
    <servlet-name>Hpolitics</servlet-name>
    <servlet-class>Help.PoliticServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Hpolitics</servlet-name>
    <url-pattern>/Hpolitics</url-pattern>
  </servlet-mapping>
</web-app>